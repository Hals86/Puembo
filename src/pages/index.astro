        <noscript
            ><img
                height="1"
                width="1"
                style="display:none"
                src={`https://www.facebook.com/tr?id=${META_PIXEL_ID}&ev=PageView&noscript=1`}
            /></noscript
        >

        <!-- Analytics: TikTok Pixel -->
